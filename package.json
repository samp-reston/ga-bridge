{
  "name": "samp-reston/ga-bridge",
  "version": "1.0.0",
  "description": "A easy-to-use bridge between your GA properties and your code.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "run": "node dist/src/index.js",
    "build:watch": "tsc --watch",
    "run:watch": "node dist/src/index.js --watch dist",
    "start": "npm-run-all --parallel build:watch run:watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samp-reston/ga-bridge.git"
  },
  "keywords": [
    "google analytics",
    "ga4"
  ],
  "author": "Samuel Preston",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/samp-reston/ga-bridge/issues"
  },
  "homepage": "https://github.com/samp-reston/ga-bridge#readme",
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/source-map-support": "^0.5.6",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "source-map-support": "^0.5.21"
  }
}
